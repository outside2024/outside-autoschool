{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "(rm -rf build || :) && (rm -rf node_modules/* || :)",
    "eslint": "eslint \"**/*.{js,jsx}\"",
    "eslint-fix": "eslint \"**/*.{js,jsx}\" --fix",
    "prettier": "prettier --check . --ignore-path ./.prettierignore",
    "prettier:fix": "prettier . --write --ignore-path ./.prettierignore",
    "freshinstall": "npm run clean && npm i --legacy-peer-deps",
    "pretest": "npm run prettier && npm run eslint && npm run test:coverage-silent && npm run cypress:all",
    "prepare": "husky install",
    "pre-push": "npm run prettier && npm run eslint",
    "test": "jest --watch",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:coverage-silent": "npm test -- --coverage --watchAll=false --silent",
    "cypress": "cypress open",
    "cypress:all": "cypress run"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.5.1",
    "formik": "^2.4.5",
    "next": "13.5.5",
    "next-i18next": "^14.0.3",
    "prop-types": "^15.8.1",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^8.1.3",
    "styled-components": "^6.1.0",
    "swiper": "^11.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "cypress": "^13.3.0",
    "cypress-fail-fast": "^7.0.3",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.5.5",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-router-mock": "^0.9.10",
    "prettier": "^3.0.3",
    "redux-mock-store": "^1.5.4"
  }
}
